{
  "name": "bolted-spanel",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cd frontend && pnpm dev",
    "build": "cd frontend && pnpm build",
    "preview": "cd frontend && pnpm preview",
    "backend": "cd backend && python main.py",
    "backend:dev": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "install:frontend": "cd frontend && pnpm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "check": "cd frontend && pnpm check",
    "lint": "cd frontend && pnpm lint",
    "format": "cd frontend && pnpm format",
    "clean": "cd frontend && rm -rf .svelte-kit dist node_modules && cd ../backend && rm -rf __pycache__"
  },
  "workspaces": [
    "frontend"
  ],
  "packageManager": "pnpm@9.0.0"
} 